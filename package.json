{
  "name": "fu-dialog",
  "version": "1.1.1",
  "description": "fisker's dialog",
  "keywords": [
    "alert",
    "confirm",
    "dialog",
    "promise",
    "prompt"
  ],
  "homepage": "https://github.com/fisker/fu-dialog#readme",
  "bugs": {
    "url": "https://github.com/fisker/fu-dialog/issues"
  },
  "repository": "fisker/fu-dialog",
  "funding": "https://github.com/fisker/fu-dialog?sponsor=1",
  "license": "MIT",
  "author": {
    "name": "fisker Cheung",
    "email": "lionkay@gmail.com",
    "url": "https://www.fiskercheung.com/"
  },
  "sideEffects": false,
  "type": "module",
  "exports": {
    "import": "./dist/full-version/dialog.mjs",
    "require": "./dist/full-version/dialog.cjs"
  },
  "main": "dist/full-version/dialog.cjs",
  "umd:main": "dist/full-version/dialog.min.js",
  "jsdelivr": "dist/full-version/dialog.min.js",
  "unpkg": "dist/full-version/dialog.min.js",
  "module": "dist/full-version/dialog.min.mjs",
  "browser": "dist/full-version/dialog.min.js",
  "style": "dist/full-version/dialog.min.css",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "run-p build:*",
    "build:css": "sh ./scripts/build-css.sh",
    "build:docs": "parcel build demo/index.html --out-dir docs",
    "build:js": "sh ./scripts/build-js.sh",
    "build:package": "sh ./scripts/build-package.sh",
    "build:styles": "sh ./scripts/build-styles-source.sh",
    "clean": "run-p clean:*",
    "clean:dist": "del-cli dist",
    "clean:docs": "del-cli docs",
    "dev": "parcel demo/index.html --open",
    "dist": "run-p dist:*",
    "dist:npm": "np --yolo --no-yarn",
    "format": "run-p format:*",
    "format:eslint": "yarn lint:eslint --fix",
    "format:markdown": "yarn lint:markdown --fix",
    "format:package-json": "sort-package-json \"package.json\" \"packages/*/package.json\"",
    "format:prettier": "yarn lint:prettier --write",
    "format:stylelint": "yarn lint:stylelint --fix",
    "lint": "run-p lint:*",
    "lint:eslint": "eslint \"**/*.{js,mjs,cjs,vue}\"",
    "lint:markdown": "markdownlint \"**/*.md\" --ignore \"**/node_modules/**\"",
    "lint:package-json": "yarn run format:package-json --check",
    "lint:prettier": "prettier \"**/*.{css,html,js,cjs,mjs,json,less,md,scss,ts,vue,yaml,yml}\" --check",
    "lint:stylelint": "stylelint \"**/*.{css,scss,less}\"",
    "release": "run-s clean lint build test format dist",
    "test": "jest",
    "test-coverage": "jest --coverage",
    "version": "yarn build && git add dist"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@babel/core": "7.20.2",
    "@babel/preset-env": "7.20.2",
    "@commitlint/cli": "17.2.0",
    "@fisker/commitlint-config": "2.0.5",
    "@fisker/eslint-config": "10.2.4",
    "@fisker/husky-config": "4.1.2",
    "@fisker/lint-staged-config": "3.1.5",
    "@fisker/postcss-config": "1.2.9",
    "@fisker/prettier-config": "5.0.6",
    "@fisker/stylelint-config": "5.0.17",
    "@rollup/plugin-babel": "6.0.2",
    "@rollup/plugin-commonjs": "23.0.2",
    "@rollup/plugin-json": "5.0.1",
    "@rollup/plugin-node-resolve": "15.0.1",
    "babel-plugin-transform-es3-member-expression-literals": "6.22.0",
    "babel-plugin-transform-es3-property-literals": "6.22.0",
    "babel-plugin-transform-es5-property-mutators": "6.24.1",
    "cp-file": "10.0.0",
    "create-banner": "2.0.0",
    "cssnano-cli": "1.0.5",
    "cz-conventional-changelog": "3.3.0",
    "del-cli": "5.0.0",
    "dialog-polyfill": "0.5.6",
    "es6-promise": "4.2.8",
    "eslint": "8.27.0",
    "esm-utils": "4.1.0",
    "gzip-size": "7.0.0",
    "husky": "8.0.2",
    "jest": "29.3.1",
    "jsdom": "20.0.2",
    "lint-staged": "13.0.3",
    "markdownlint-cli": "0.32.2",
    "mem": "9.0.2",
    "npm-run-all": "4.1.5",
    "parcel-bundler": "1.12.5",
    "postcss": "8.4.19",
    "postcss-cli": "10.0.0",
    "postcss-header": "3.0.3",
    "postcss-pseudo-element-colons": "1.0.2",
    "prettier": "2.7.1",
    "prettier-config-fisker": "1.0.3",
    "pretty-bytes": "6.0.0",
    "read-dir-deep": "7.0.1",
    "rollup": "3.29.5",
    "rollup-plugin-prettier": "3.0.0",
    "rollup-plugin-terser": "7.0.2",
    "sass": "1.56.1",
    "sort-package-json": "2.1.0",
    "stylelint": "16.0.2",
    "table": "6.8.1",
    "uglify-js": "3.17.4",
    "write-pkg": "5.1.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
